extends layout

block content
  h2 Recent Callbacks
  p.
    A list of all raw webhook callbacks.
  form(action="/admin/delete_callbacks" method="post")
    button(type="submit") Clear

  table.callbacks
    thead
      tr
        th Time
        th Headers
        th Body
    tbody
      each callback in callbacks
        tr
          td= moment(callback.createdAt).fromNow()
          td
            pre= JSON.stringify(callback.headers, null, 1)
          td
            pre= JSON.stringify(callback.body, null, 1)
