{
  "name": "Pusheen Suite",
  "description": "This app does everything.",
  "keywords": [
    "workplace",
    "pusheen",
    "node"
  ],
  "website": "https://github.com/jokr/pusheen-suite",
  "repository": "https://github.com/jokr/pusheen-suite",
  "logo": "http://thecraftchop.com/files/others/pusheen.svg",
  "success_url": "/",
  "env": {
    "APP_ID": {
      "description": "App ID from Dev Dashboard.",
      "value": "9999999999999999"
    },
    "APP_SECRET": {
      "description": "App Secret from Dev Dashboard.",
      "value": "0123456789abcdef"
    },
    "APP_REDIRECT": {
      "description": "todo",
      "value": "https://XXXX.herokuapp.com/community_install"
    },
    "SESSION_SECRET": {
      "description": "todo",
      "generator": "random"
    },
    "WEBHOOK_CALLBACK": {
      "description": "todo",
      "value": "https://XXXX.herokuapp.com/api/webhook"
    },
    "SALT_ROUNDS": {
      "description": "Number of bcrypt rounds.",
      "value": "10"
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "image": "heroku/nodejs",
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev"
    }
  ]
}
